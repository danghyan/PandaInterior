        <script>
            function calcSum() {
                if ($('#telcalc').val() == '' || $('#telcalc').val() == 'Undefined') {
                    $('#alert').show();
                    return;
                }
                else {
                    $('#alert').hide();

                }
                sendCallback('CALC', $('#telcalc').val(), '', 0)

                var reg = $('#browsers').val();
                var quan = $('#ploshad').val();

                if (reg == 'Квартира') {
                    if (quan < 65) {
                        var coef = Math.random();
                        $('#1').val(80 + '.000 рублей');
                    }
                    if (quan > 65 && quan < 80) {
                        var coef = Math.random();
                        $('#1').val(110 + '.000 рублей');
                    }
                    if (quan > 80 && quan < 110) {
                        var coef = Math.random();
                        $('#1').val(130 + '.000 рублей');
                    }
                    if (quan > 110 && quan < 150) {
                        var coef = Math.random();
                        $('#1').val(170 + '.000 рублей');
